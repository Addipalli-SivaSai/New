pipeline {
    agent any

    stages {
         stage('show-content')
         {
             steps{
                 bat 'echo %cd%'
                 bat 'dir'
                 bat 'type first.txt'
             }
         }
         
    
         }
         post{
             success{
               bat'git checkout -b main'
               bat 'git merge sai_new'
echo "All the above stages are successfull"
             }
         }
    
}
